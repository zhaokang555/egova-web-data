{
    "compilerOptions": 
    {
        "module": "es2015",
        "target": "es5",
        "moduleResolution": "node",
        "allowJs": true,
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "src/*": ["./src/*"],
            "components/*": ["./src/components/*"],
            "models/*": ["./src/models/*"],
            "views/*": ["./src/views/*"],
            "@iview/*": ["./node_modules/flagwind-web/dist/typings/components/iview/typings/*"]
        },
        "lib": 
        [
            "dom",
            "es5",
            "es2015",
            "es2015.promise"
        ]
    }
}